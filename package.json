		"@sveltejs/adapter-cloudflare": "^4.7.4",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/bcryptjs": "^2.4.6",
		"@types/eslint": "^9.6.0",
		"autoprefixer": "^10.4.20",
		"bcryptjs": "^2.4.3",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"jose": "^5.9.6",
