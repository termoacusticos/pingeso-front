		"tailwindcss": "^3.4.9",
		"tailwindcss-animated": "^1.1.2",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.0.3",
		"vitest": "^2.0.4",
		"wrangler": "^3.95.0"
	},
	"dependencies": {
		"@prisma/adapter-d1": "^6.0.1",
		"@prisma/client": "^6.1.0",
		"pdf-lib": "^1.17.1"
